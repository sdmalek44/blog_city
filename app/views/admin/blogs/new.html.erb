<h1>Admin Create Blog</h1>
<%= form_for @blog do |f| %>
<%= f.collection_check_boxes :categories, Category.all, :id, :name %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :blurb %>
<%= f.text_area :blurb %>
<%= f.label :body %>
<%= f.text_area :body %>
<%= f.label :image %>
<%= f.text_field :image %>
<%= f.submit %>
<% end %>
